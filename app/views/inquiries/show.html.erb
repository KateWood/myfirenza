<div class="container-fluid">

	<h1><%= @inquiry.first_name %> <%= @inquiry.last_name %>'s Inquiry</h1>
	<div cl"table-responsive">
		<table class="table table-hover">
			<thead>
			</thead>
			<tbody>
				<tr><td><strong>Date of Inquiry:</strong></td><td> <%= @inquiry.created_at.strftime("%b %d, %Y") %></td></tr>
				<tr><td><strong>Name:</strong></td><td> <%= @inquiry.first_name %> <%= @inquiry.last_name %></td></tr>
				<tr><td><strong>Email Address:</strong></td><td> <%= mail_to @inquiry.email %></td></tr>
				<tr><td><strong>Phone Number:</strong></td><td> <%= @inquiry.phone %></td></tr>
				<tr><td><strong>City of Interest:</strong></td><td> <%= @inquiry.city_of_interest %>, <%= @inquiry.state %> <%= @inquiry.zip %></td></tr>
				<tr><td><strong>Reason for Interest:</strong></td><td> <%= @inquiry.reason %></td></tr>
				<tr><td><strong>Net Worth:</strong></td><td> <%= @inquiry.networth %></td></tr>
				<tr><td><strong>Liquidity:</strong></td><td> <%= @inquiry.liquidity %></td></tr>
			</tbody>
		</table>
	</div>
	<%= link_to 'Back', inquiries_path %>

</div>


