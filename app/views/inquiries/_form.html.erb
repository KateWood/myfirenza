<%= form_for(@inquiry) do |f| %>
	<div class="form-group shrink">
		<%= f.label :first_name %>
		<%= f.text_field :first_name %>
	</div>
	<div class="form-group shrink">
		<%= f.label :last_name %>
		<%= f.text_field :last_name %>
	</div>
	<div class="form-group shrink">
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>
	<div class="form-group shrink">
		<%= f.label :phone %>
		<%= f.text_field :phone %>
	</div>
	<div class="form-group shrink">
		<%= f.label :city_of_interest %>
		<%= f.text_field :city_of_interest %>
	</div>
	<div class="form-group shrink">
		<%= f.label :state %>
		<%= f.text_field :state %>
	</div>
	<div class="form-group shrink">
		<%= f.label :zip %>
		<%= f.number_field :zip %>
	</div>
	<div class="form-group shrink">
		<%= f.label :reason %>
		<%= f.text_area :reason %>
	</div>
	<div class="form-group shrink">
		<%= f.label :networth %>
		<%= f.number_field :networth %>
	</div>
	<div class="form-group shrink">
		<%= f.label :liquidity %>
		<%= f.number_field :liquidity %>
	</div>
	<div class="shrink">
		<%= f.submit %>
	</div>
<% end %>



